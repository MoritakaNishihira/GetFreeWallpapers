  // GetFreeWallpapers専用JSONスニペット
  
  "ToolSettings Template": {
    "prefix": "toolsettings",
    "body": [
      "{",
      "  \"settings\": {",
      "    \"count_per_theme\": ${1:5},",
      "    \"resolution\": \"${2|thumb,small,regular,full,raw|}\",",
      "    \"orientation\": \"${3|landscape,portrait,squarish|}\"",
      "  },",
      "  \"themes\": [",
      "    {",
      "      \"name\": \"${4:テーマ名}\",",
      "      \"query\": \"${5:検索キーワード}\",",
      "      \"enabled\": ${6|true,false|}",
      "    }$0",
      "  ]",
      "}"
    ],
    "description": "ToolSettings.json の基本テンプレート"
  },
  
